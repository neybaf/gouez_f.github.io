      <paragraphe valeur_ptsodj="0" ordinal_prise="1" id_preparation="2410132" ordre_absolu_seance="2" id_acteur="PA721908" id_mandat="PM797568" id_nomination_oe="-1" id_nomination_op="PM800479" code_grammaire="OUV_SEAN_2_1" code_style="NORMAL" code_parole="" sommaire="0" id_syceron="3314050" valeur="" roledebat="president">
